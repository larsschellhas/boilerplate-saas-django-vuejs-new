{
 "processors": [],
 "extends": [
   "stylelint-config-standard-scss",
   "stylelint-config-recommended-vue"
  ],
 "ignoreFiles": [
   "node_modules",
    "dist/**"
  ],
 "rules": {
   "color-hex-case": [
     "lower",
     {
       "message": "Lowercase letters are easier to distinguish from numbers"
     }
   ],
   "at-rule-no-unknown": null,
   "scss/at-rule-no-unknown": true,
   "indentation": 2
 }
}